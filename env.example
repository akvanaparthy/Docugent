# Vercel AI Gateway - Primary option (requires credit card on file)
# Get this from your Vercel dashboard under AI Gateway settings
AI_GATEWAY_API_KEY=your_ai_gateway_api_key_here

# OpenAI API Key - Fallback option (if AI Gateway requires credit card)
# Get this from https://platform.openai.com/api-keys
# OPENAI_API_KEY=your_openai_api_key_here

# Application settings
NEXT_PUBLIC_APP_URL=http://localhost:3000

# LM Studio settings (for local development)
LM_API_KEY=lmstudio
LM_BASE_URL=http://127.0.0.1:1234/v1
LM_MODEL=dolphin-2.9.3-mistral-nemo-12b-llamacppfixed:2

# LM Studio settings (for production - replace with your public IP or ngrok URL)
# LM_BASE_URL=http://your-public-ip:1234/v1
# LM_BASE_URL=https://8bc78ab669c5.ngrok-free.app/v1

# Embedding settings (set to true to disable embeddings and use text-based similarity)
DISABLE_EMBEDDINGS=true

# Model loading optimization (set to true to prevent JIT loading)
KEEP_MODELS_LOADED=true
